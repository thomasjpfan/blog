if (!document.getElementById('katexscript_pelican_#%@#$@#')) {{
    var katexscript = document.createElement('script');
    katexscript.id = 'katexscript_pelican_#%@#$@#';
    katexscript.type = 'text/javascript';
    katexscript.src = 'https://cdnjs.cloudflare.com/ajax/libs/KaTeX/0.7.1/katex.min.js';
    katexscript.integrity = 'sha384-/y1Nn9+QQAipbNQWU65krzJralCnuOasHncUFXGkdwntGeSvQicrYkiUBwsgUqc1';
    katexscript.crossOrigin = 'anonymous';
    (document.body || document.getElementsByTagName('head')[0]).appendChild(katexscript);
}}

if (!document.getElementById('katexscript_pelican_css_#%@#$@#')) {{
    var katex_css = document.createElement('link');
    katex_css.rel = 'stylesheet';
    katex_css.href = 'https://cdnjs.cloudflare.com/ajax/libs/KaTeX/0.7.1/katex.min.css';
    katex_css.integrity = 'sha384-wITovz90syo1dJWVh32uuETPVEtGigN07tkttEqPv+uR2SE/mbQcG7ATL28aI9H0'
    katex_css.crossOrigin= 'anonymous';
    (document.body || document.getElementsByTagName('head')[0]).appendChild(katex_css);
}}

if (!document.getElementById('katexscript_pelican_auto_render_#%@#$@#')) {{
    var katex_ar_script = document.createElement('script');
    katex_ar_script.id = 'katexscript_pelican_auto_render_#%@#$@#';
    katex_ar_script.type = 'text/javascript';
    katex_ar_script.src = 'https://cdnjs.cloudflare.com/ajax/libs/KaTeX/0.7.1/contrib/auto-render.min.js';
    katex_ar_script.integrity = 'sha384-dq1/gEHSxPZQ7DdrM82ID4YVol9BYyU7GbWlIwnwyPzotpoc57wDw/guX8EaYGPx';
    katex_ar_script.crossOrigin = 'anonymous';
    katex_ar_script[(window.opera ? "innerHTML": "text")] =
        "renderMathInElement(" +
        "document.getElementById('test')," +
        "{{ delimiters: [" +
        "{{left: '$$', right: '$$', display: true}}," +
        "{{left: '\\[', right: '\\]', display: true}}," +
        "{{left: '$', right: '$', display: false}}," +
        "{{left: '\\(', right: '\\)', display: false}}]}}";
    (document.body || document.getElementsByTagName('head')[0]).appendChild(katex_ar_script);
}}

